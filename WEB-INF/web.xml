<?xml version="1.0" encoding="UTF-8"?>
<web-app
	version="3.0"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
	                    http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
	<servlet>
		<servlet-name>Login</servlet-name>
		<servlet-class>com.qubo.caea05.servlets.LoginServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Logout</servlet-name>
		<servlet-class>com.qubo.caea05.servlets.LogoutServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Menu</servlet-name>
		<servlet-class>com.qubo.caea05.servlets.MenuServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Select</servlet-name>
		<servlet-class>com.qubo.caea05.servlets.SelectServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>MultiCommand</servlet-name>
		<servlet-class>com.qubo.caea05.servlets.MultiCommandServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Insert</servlet-name>
		<servlet-class>com.qubo.caea05.servlets.InsertServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>InsertConfirm</servlet-name>
		<servlet-class>com.qubo.caea05.servlets.InsertConfirmServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Update</servlet-name>
		<servlet-class>com.qubo.caea05.servlets.UpdateServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>UpdateInput</servlet-name>
		<servlet-class>com.qubo.caea05.servlets.UpdateInputServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>UpdateConfirm</servlet-name>
		<servlet-class>com.qubo.caea05.servlets.UpdateConfirmServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Delete</servlet-name>
		<servlet-class>com.qubo.caea05.servlets.DeleteServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>DeleteConfirm</servlet-name>
		<servlet-class>com.qubo.caea05.servlets.DeleteConfirmServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Lump</servlet-name>
		<servlet-class>com.qubo.caea05.servlets.LumpServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Error</servlet-name>
		<servlet-class>com.qubo.caea05.servlets.ErrorServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>Login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Logout</servlet-name>
		<url-pattern>/logout</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Menu</servlet-name>
		<url-pattern>/menu</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>MultiCommand</servlet-name>
		<url-pattern>/multi</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Select</servlet-name>
		<url-pattern>/select</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Insert</servlet-name>
		<url-pattern>/insert</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>InsertConfirm</servlet-name>
		<url-pattern>/insertConfirm</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Update</servlet-name>
		<url-pattern>/update</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>UpdateInput</servlet-name>
		<url-pattern>/updateInput</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>UpdateConfirm</servlet-name>
		<url-pattern>/updateConfirm</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Delete</servlet-name>
		<url-pattern>/delete</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>DeleteConfirm</servlet-name>
		<url-pattern>/deleteConfirm</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Lump</servlet-name>
		<url-pattern>/lump</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Error</servlet-name>
		<url-pattern>/error*</url-pattern>
	</servlet-mapping>

	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
	<error-page>
		<error-code>404</error-code>
		<location>/error404</location>
	</error-page>
	<error-page>
		<error-code>405</error-code>
		<location>/error405</location>
	</error-page>
</web-app>